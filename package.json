{
  "name": "fullstack-startcode",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node build/app.js",
    "dev": "nodemon src/bin/www.ts",
    "dev:debug": "nodemon --inspect src/bin/www.ts",
    "test": "mocha --exit  -r ts-node/register test/**/*.ts",
    "test2": "mocha --exit  -r ts-node/register playground/what/whattodoTest.ts",
    "test3": "mocha --exit  -r ts-node/register playground/nameinfo/nameInfoTest.ts",
    "test:debug": "mocha --exit -r ts-node/register --inspect test/**/*.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "test/**/*",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node -r ts-node/register"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "mongodb": "^3.6.5",
    "mongodb-memory-server": "^6.9.6",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "tsscmp": "^1.0.6",
    "typescript": "^4.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^8.2.2",
    "@types/mongodb": "^3.6.11",
    "@types/morgan": "^1.9.2",
    "@types/nock": "^11.1.0",
    "@types/node-fetch": "^2.5.10",
    "@types/supertest": "^2.0.11",
    "@types/tsscmp": "^1.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "mocha": "^8.3.2",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1"
  }
